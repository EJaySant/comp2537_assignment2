<%- include("templates/header") %>
    <div class="container my-5">
        <div class="container">
            <h1 class="text-center my-2">Hello, <%= name %>!</h1>
            <div class="row justify-content-center mx-auto" style="width:100%">
                <% membersMedia.forEach(media => { %>
                    <%- include("templates/membersImg", {media: media}) %>
                <% }); %>
            </div>
        </div>
        <form method="get" action="/logout" class="input-group my-3">
            <button class="btn btn-outline-secondary">Logout</button>
        </form>
    </div>
<%- include("templates/footer") %>